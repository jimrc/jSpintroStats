{
  "svg": {
    "width": "80%",
    "height": "80%"
  },
  "plot_attributes": {
    "title": {
      "family": "Helvetica",
      "size": 20
    },
    "tooltip": {
      "curve": 5,
      "point": 20,
      "fill": "rgba(10,10,10,.2)",
      "stroke": "#ffffff",
      "opacity": 0.8,
      "text": "black",
      "emphasis": "red",
      "family": "Helvetica",
      "size": 14,
      "default_separation_from_object": 3
    },
    "axes": {
      "family": "Helvetica",
      "size": 10,
      "ticks": {
        "size": 5
      },
      "maxCharacters": {
        "x": 10
      },
      "labels": {
        "family": "Helvetica",
        "size": 14
      }
    },
    "buttons": {
      "family": "Courier New",
      "size": 12,
      "stroke": "black",
      "fill": {
        "not_selected": "white",
        "selected": "black"
      }
    }
  },
  "plots": {
    "scatter": {
      "point": {
        "radius": 5
      },
      "opacity": {
        "hover": 0.5,
        "nonhover": 0.8
      }
    },
    "line": {
      "width": 5,
      "stroke": "#2196F3",
      "fill": "none",
      "hidden_radius": 10,
      "opacity": {
        "hover": 0.5,
        "nonhover": 0.8
      }
    }
  }
}
